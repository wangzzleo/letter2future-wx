
.header-box{
  width: 100%;
  height: 140rpx; 
  padding: 15rpx 20rpx;
  border-bottom: 1px solid #efefef;
}
.header-text{
  display: inline-block;
  height: 140rpx;
  line-height: 140rpx;
  float: left;
}
.cellBox{
  height: 90rpx;
  line-height: 90rpx;
  width: 100%;
  padding: 0 30rpx;
  font-family:  Arial, Helvetica, sans-serif;
  font-size: 30rpx;
  color: #707070;
  border-bottom: 1px solid #efefef;
}
.cellBox-img{
   width: 50rpx;
    height: 50rpx;
    float: left;
    margin: 16rpx 15rpx 0 0 ;
}
.cellBox-icon{
  width: 35rpx;
  height: 35rpx;
  float: right;
  margin: 27rpx 60rpx 0 0 ;
}